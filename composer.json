{
    "name": "chine/laravel-uniform-response",
    "description": "A Laravel package for uniform JSON responses.",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "chineyoung",
            "email": "chineyoung2024@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2|^8.3|^8.4",
        "laravel/framework": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "9.9.0"
    },
    "autoload": {
        "psr-4": {
            "Chine\\UniformResponse\\": "src/"
        },
        "autoload-dev": {
            "psr-4": {
                "Tests\\": "tests/"
            }
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        },
        "laravel": {
            "providers": [
                "Chine\\UniformResponse\\UniformResponseServiceProvider"
            ],
            "aliases": {
                "Response": "Chine\\UniformResponse\\Facades\\ResponseFacade"
            }
        }
    }
}
